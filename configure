		DST_PRIVATEOPENSSL=''
		dst_privateopenssl=''
		DST_OPENSSL_INC="-I$use_openssl/include"
		DNS_OPENSSL_LIBS="-L$use_openssl/lib -lcrypto"
		DST_OPENSSL_LIB=''
		echo "$ac_t""using openssl from $use_openssl/lib and $use_openssl/include" 1>&6
		;;
esac







