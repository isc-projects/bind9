options {
	query-source address 10.53.0.1;
	notify-source 10.53.0.1;
	transfer-source 10.53.0.1;
	port @PORT@;
	pid-file "named.pid";
	listen-on { 10.53.0.1; };
	listen-on-v6 { none; };
	recursion no;
	notify yes;
};

zone "include" {
	type master;
	file "include.db";
};

zone "ttl1" {
	type master;
	file "ttl1.db";
};

zone "ttl2" {
	type master;
	file "ttl2.db";
};
