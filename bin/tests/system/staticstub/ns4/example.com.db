$ORIGIN .
$TTL 300	; 5 minutes
example.com		IN SOA	example. hostmaster.example. (
				2010080701 ; serial
				20         ; refresh (20 seconds)
				20         ; retry (20 seconds)
				1814400    ; expire (3 weeks)
				3600       ; minimum (1 hour)
				)
example.com.		NS	example.

data.example.com.	TXT	"example com data"
