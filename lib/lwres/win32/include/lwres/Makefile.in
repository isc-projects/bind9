srcdir =	@srcdir@
VPATH =		@srcdir@
top_srcdir =	@top_srcdir@

HEADERS =	net.h
SUBDIRS =
TARGETS =

@BIND9_MAKE_RULES@

installdirs:
	$(SHELL) ${top_srcdir}/mkinstalldirs ${DESTDIR}${includedir}/lwres

install:: installdirs
	for i in ${HEADERS}; do \
		${INSTALL_DATA} $(srcdir)/$$i ${DESTDIR}${includedir}/lwres ; \
	done
